<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd" >

    <bean id="foo" class="org.faoxis.spring4pro.chapter3.autowire.Foo" />
    <bean id="bar2" class="org.faoxis.spring4pro.chapter3.autowire.Bar" />

    <bean id="targetByName" autowire="byName"
          class="org.faoxis.spring4pro.chapter3.autowire.Target"
          lazy-init="true" />

    <bean id="targetByType" autowire="byType"
          class="org.faoxis.spring4pro.chapter3.autowire.Target"
          lazy-init="true" />

    <bean id="targetConstructor" autowire="constructor"
          class="org.faoxis.spring4pro.chapter3.autowire.Target"
          lazy-init="true" />


</beans>

